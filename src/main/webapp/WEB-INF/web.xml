<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


<servlet>
	<servlet-name>CustomerInsert</servlet-name>
	<servlet-class>com.customer.CustomerInsert</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>CustomerInsert</servlet-name>
	<url-pattern>/insert</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>bookeventServlet</servlet-name>
	<servlet-class>com.customerevent.bookeventServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>bookeventServlet</servlet-name>
	<url-pattern>/eventInsert</url-pattern>
</servlet-mapping>
         
<servlet>
    <servlet-name>AddEvent</servlet-name>
    <servlet-class>Admin.AddEventControl</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>AddEvent</servlet-name>
    <url-pattern>/Add</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>VendorInsert</servlet-name>
    <servlet-class>com.vendor.VendorInsert</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>VendorInsert</servlet-name>
    <url-pattern>/insertV</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>Create_Advertisement</servlet-name>
	<servlet-class>Vendor.AdServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Advertisement insert</servlet-name>
	<url-pattern>/adinsert</url-pattern>
</servlet-mapping>


</web-app>